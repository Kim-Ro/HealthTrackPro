{
  "name": "express",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "express/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/express",
        "main": "express/src/main.js",
        "tsConfig": "express/tsconfig.app.json",
        "assets": ["express/src/assets"],
        "webpackConfig": "express/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "express:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "express:build:development"
        },
        "production": {
          "buildTarget": "express:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "express/jest.config.js"
      }
    }
  },
  "tags": []
}
